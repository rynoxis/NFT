{
  "name": "astar-examples",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "10.1.0",
    "ts-node": "^10.8.0",
    "@supercolony/typechain-compiler": "^0.5.1-beta.1",
    "@supercolony/typechain-types": "^0.0.16",
    "patch-package": "^6.4.7"
  },
  "scripts": {
    "build:release": "npx redspot compile --release",
    "build": "npx redspot compile",
    "compile": "typechain-compiler",
    "compile:release": "typechain-compiler --release",
    "test:typechain": "mocha --require ts-node/register --recursive ./tests --extension \".spec.ts\" --exit --timeout 20000",
    "test:redspot": "npx redspot test",
    "test:mocha": "mocha --require @babel/register --require ts-node/register --recursive ./tests --extension \".ts\" --exit --timeout 20000",
    "test:mocha-single": "mocha --require @babel/register --require ts-node/register --extension \".ts\" --exit --timeout 20000",
    "test": "yarn build:release && yarn test:mocha",
    "postinstall": "patch-package"
  },
  "resolutions": {
    "@polkadot/api": "^9.6.1",
    "@polkadot/api-contract": "^9.6.1"
  }
}
